{
  "manifest_version": 3,
  "name": "Chess.com moves Extractor",
  "version": "1.0",
  "description": "Open any chess.com game and click on the extension to extract the moves",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}